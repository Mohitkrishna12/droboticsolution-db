<%- include("header.ejs") -%>
<body>
   <header class="site-navbar py-4 js-sticky-header site-navbar-target" role="banner">

      <div class="container">
        <div class="row align-items-center">
          
          <div class="col-6 col-md-3 col-xl-4  d-block">
            <h1 class="mb-0 site-logo"><a href="index.html" class="text-black h2 mb-0"><img src="images/logo.png" alt="imag"></a></h1>
          </div>

         


         
        </div>
      </div>
      
    </header>
  <h1 class="text-c" style="padding: 130px 0 50px 0">Contact Forms</h1>

  <div class="container" >
    <div class="row">
      <div class="col-sm-1">
        <table width="1110px" border="1px" align="center" >
        <th class="bg-primary th-c"> Date</th>
        <th class="bg-primary th-c"> First Name</th>
        <th class="bg-primary th-c">Last Name</th>
        <th class="bg-primary th-c">Email</th>
        <th class="bg-primary th-c">Subject</th>
        <th class="bg-primary th-c">Message</th>



    
        <% for(var i=0;i<formname.length;i++){  %>
            
             

              <tr class="" align="center">
                <td><p class="text-black th-d"><%= formname[i].date %></p><br></td>


                <td><p class="text-black th-d"><%= formname[i].fname %></p><br></td>

              
                <td>
                  <p class="text-black th-d"><%= formname[i].lname %></p><br>
                  
                </td>
                 <td>
                  <p class="text-black th-d"><%= formname[i].email %></p><br>
                  
                </td>
                 <td>
                  <p class="text-black th-d"><%= formname[i].subject %></p><br>
                  
                </td>
                 <td>
                  <p class="text-black th-d"><%= formname[i].message %></p><br>
                  
                </td>
              
              </tr>
              
              




           
            

         <%}%>
          </table>
    
          
          
            
        
      </div>
    </div>
    

        
        
          
        
        
        
    

        
  </div>
</body>
</html>